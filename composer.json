{
    "name": "szemul/slim-error-handler-bridge",
    "description": "Slim-error handler bridge",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.52.0",
        "psr/http-server-middleware": "^1.0.1",
        "ramsey/uuid": "^3.0|^4.0",
        "slim/slim": "^4.11",
        "szemul/error-handler": "^v1.0.0|^2.0.0",
        "szemul/not-set-value": "^v2.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\SlimErrorHandlerBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\SlimErrorHandlerBridge\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
